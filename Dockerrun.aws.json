{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "client",
        "image": "dandu1008/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
        "name": "server",
        "image": "dandu1008/multi-server",
        "hostname": "api",
        "essential": false,
        "memory": 128
      },
      {
        "name": "worker",
        "image": "dandu1008/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "dandu1008/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [
            {
              "hostPort": 80,
              "containerPort": 80
            }
          ],
          "links": ["client", "server"]
      }
    ]
}